<core:FragmentDefinition xmlns="sap.m" xmlns:commons="sap.suite.ui.commons" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
    <commons:Timeline
			id="idTimeline"
			enableDoubleSided="true"
			groupBy="HireDate"
			growingThreshold="0"
			filterTitle="Job Title"
			sortOldestFirst="true"
			content="{ViewModel>/AppObject/appClients}">
		<commons:content>
			<commons:TimelineItem visible ="{= ${ViewModel>delete} ? false : true}"
					id="idTemplateItem"
					dateTime="01 Jan 2021"
					title="{ViewModel>ClientName}"
					userNameClickable="true"
					userNameClicked="onUserNameClick"
					select="onPressItems"
					userPicture="sap-icon://customer"
					text="{ViewModel>Feedback}"
					userName="{ViewModel>ClientName}"
					filterValue="{ViewModel>EndUsers}"
					icon="sap-icon://activity-individual">
			</commons:TimelineItem>
		</commons:content>
	</commons:Timeline>
    </VBox>
</core:FragmentDefinition>